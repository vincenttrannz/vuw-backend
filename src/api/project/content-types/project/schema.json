{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "project",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "ProjectTitle": {
      "type": "string",
      "required": true
    },
    "ProjectDescription": {
      "type": "richtext",
      "required": true
    },
    "LecturerName": {
      "type": "string"
    },
    "CourseName": {
      "type": "string",
      "required": true
    },
    "ProjectDate": {
      "type": "date",
      "required": true
    },
    "ProjectExternalLink": {
      "type": "string"
    },
    "ProjectLinkDisplay": {
      "type": "string"
    },
    "ImagesCarousel": {
      "type": "boolean",
      "default": false
    },
    "ProjectImages": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "Project3D": {
      "type": "boolean",
      "default": false
    },
    "Project3DLink": {
      "type": "string"
    },
    "SeoData": {
      "type": "component",
      "repeatable": false,
      "component": "seo.seo-data"
    },
    "student": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::student.student",
      "inversedBy": "projects"
    },
    "ProjectThumbnail": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "Slug": {
      "type": "string",
      "required": false
    },
    "ProjectPDF": {
      "type": "boolean",
      "default": false
    },
    "ProjectPDFLink": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "ProjectCode": {
      "type": "boolean",
      "default": false
    },
    "ProjectCodeLink": {
      "type": "string"
    },
    "level": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::level.level",
      "inversedBy": "projects"
    },
    "major": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::major.major",
      "inversedBy": "projects"
    },
    "school": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::school.school",
      "inversedBy": "projects"
    },
    "award": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::award.award",
      "inversedBy": "projects"
    },
    "ProjectTags": {
      "type": "string"
    },
    "DownloadLinkOne": {
      "type": "string"
    },
    "DownloadLinkTwo": {
      "type": "string"
    },
    "DownloadLinkOneNameDisplay": {
      "type": "string"
    },
    "DownloadLinkTwoNameDisplay": {
      "type": "string"
    },
    "LicensingLink": {
      "type": "string"
    },
    "LicensingNameDisplay": {
      "type": "string"
    },
    "ProjectVideo": {
      "type": "boolean",
      "default": false,
      "required": false
    },
    "ProjectVideoLink": {
      "type": "string"
    },
    "SubProjectCarousel": {
      "type": "component",
      "repeatable": false,
      "component": "project.project-carousel"
    },
    "SubProject3D": {
      "type": "component",
      "repeatable": false,
      "component": "project.project3-d"
    },
    "SubProjectPDF": {
      "type": "component",
      "repeatable": false,
      "component": "project.project-pdf"
    },
    "SubProjectVideo": {
      "type": "component",
      "repeatable": false,
      "component": "project.project-video"
    },
    "SubProjectCode": {
      "type": "component",
      "repeatable": false,
      "component": "project.project-code"
    },
    "ProjectCaption": {
      "type": "string"
    }
  }
}
