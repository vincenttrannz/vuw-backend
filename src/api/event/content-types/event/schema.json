{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "EventName": {
      "type": "string",
      "required": true
    },
    "EventShortDescription": {
      "type": "text",
      "required": true
    },
    "EventLocation": {
      "type": "string"
    },
    "EventPrice": {
      "type": "string"
    },
    "EventType": {
      "type": "enumeration",
      "enum": [
        "Private",
        "Public"
      ],
      "required": true
    },
    "EventStartDate": {
      "type": "date",
      "required": true
    },
    "EventFinishDate": {
      "type": "date",
      "required": false
    },
    "EventStartTime": {
      "type": "time",
      "required": true
    },
    "EventImageThumbnail": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "EventRichDescription": {
      "type": "richtext",
      "required": true
    },
    "EventVideoLink": {
      "type": "string"
    },
    "EventPhotoGallery": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "projects": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project.project"
    },
    "EventEndTime": {
      "type": "time"
    },
    "event_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::event-category.event-category",
      "inversedBy": "events"
    },
    "EventPriceType": {
      "type": "enumeration",
      "enum": [
        "Free",
        "Ticket"
      ],
      "required": true
    },
    "EventLink": {
      "type": "string"
    }
  }
}
